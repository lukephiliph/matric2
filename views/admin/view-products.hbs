      <canvas class="my-4 w-100" id="myChart" width="900" height="380"></canvas>

      <h2>Products</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm" id="tableone">
          <thead>
            <tr>
              <th scope="col">Title</th>
              <th scope="col">Category</th>
              <th scope="col">Price</th>
              <th scope="col">Discription</th>
              <th scope="col">Image</th>
              <th scope="col">Options</th>
            </tr>
          </thead>
          <tbody>
    {{#each products}}
    <tr>
      
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>Rs.{{this.Price}} /-</td>
      <td>{{this.Description}}</td>
      <td><img style="height: 50px;width: 50px;" src="/product-images/{{this._id}}.jpg" alt=""></td>
      <td>
        <a href="/admin/edit-product/{{this._id}}" class="btn btn-primary me-3">Edit</a>
        <a href="/admin/delete-product/{{this._id}}" class="btn btn-danger " onclick="return confirm('Delete the item {{this.Name}} ?')" >Delete</a>
      
      </td>
     
    </tr>
    {{/each}}
