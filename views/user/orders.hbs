<section>
      <link rel="stylesheet" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.3.2/css/buttons.dataTables.min.css">
<link
      rel="stylesheet"
      href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css"
    />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.13.1/datatables.min.css"/>
    <div class="container mt-5">
        <table class="table table-striped mt-5" id="tablethree">
  <thead>
    <tr>
      <th scope="col">Date</th>
      <th>Address</th>
      <th >Pincode</th>
      <th >mobile</th>
      <th scope="col">Amount</th>
      <th scope="col">Payment</th>
      <th>Status</th>
      <th>View products</th>
    </tr>
  </thead>
  <tbody>
    {{#each orders}}
    <tr>
      <th>{{this.date}}</th>
      <td>{{this.deliveryDetails.address}}</td>
      <td>{{this.deliveryDetails.pincode}}</td>
      <td>{{this.deliveryDetails.mobile}}</td>
      <td>Rs. {{this.totalAmount}} /-</td>
      <td>{{this.paymentMethod}}</td>
      <td>{{this.status}}</td>
    
      
      
      
      <td>
        <a href="/view-order-products/{{this._id}}" class="btn btn-success">View Products</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>
<script>
    $(document).ready(function() {
    $('#tablethree').DataTable( {
        dom: 'Bfrtip',
        buttons: [
            'copy', 'csv', 'excel', 'pdf', 'print'
        ]
    } );
} );


    
</script>


